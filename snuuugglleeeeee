if not game:IsLoaded() then
game.Loaded:Wait()
end


loadstring(game:HttpGet("https://raw.githubusercontent.com/wimsies/wimsware/main/snuggleahaha"))()
DaHoodSettings.Prediction = 0.118
Aiming.TargetPart = {"Head", "LeftHand", "RightHand", "LeftLowerArm", "RightLowerArm", "LeftUpperArm", "RightUpperArm", "LeftFoot", "LeftLowerLeg", "UpperTorso"}
Aiming.FOV = 12
Aiming.FOVSides = 30
Aiming.HitChance = 100
Aiming.ShowFOV = false


local player = game.Players.LocalPlayer
local mouse = player:GetMouse()

mouse.KeyDown:Connect(function(key)
    if key == "z" then
        if Aiming.Enabled == false then
        Aiming.Enabled = true
        else
        Aiming.Enabled = false
        end
    end
end)

